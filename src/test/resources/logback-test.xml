<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="default" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %.-1level %logger{30} %msg%n</pattern>
		</encoder>
	</appender>

	<root level="debug">
		<appender-ref ref="default" />
	</root>

</configuration>